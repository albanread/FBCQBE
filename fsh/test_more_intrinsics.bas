PRINT "Testing ABS, SGN, MIN, MAX intrinsics:"
PRINT

PRINT "ABS(-5) ="; ABS(-5)
PRINT "ABS(5) ="; ABS(5)
PRINT "ABS(0) ="; ABS(0)
PRINT

PRINT "SGN(-5) ="; SGN(-5)
PRINT "SGN(5) ="; SGN(5)
PRINT "SGN(0) ="; SGN(0)
PRINT

PRINT "MIN(10, 20) ="; MIN(10, 20)
PRINT "MIN(20, 10) ="; MIN(20, 10)
PRINT "MIN(-5, 3) ="; MIN(-5, 3)
PRINT

PRINT "MAX(10, 20) ="; MAX(10, 20)
PRINT "MAX(20, 10) ="; MAX(20, 10)
PRINT "MAX(-5, 3) ="; MAX(-5, 3)
PRINT

PRINT "Double versions:"
PRINT "MIN(3.7, 2.1) ="; MIN(3.7, 2.1)
PRINT "MAX(3.7, 2.1) ="; MAX(3.7, 2.1)
